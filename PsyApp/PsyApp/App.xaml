<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:sharpnado="clr-namespace:Sharpnado.MaterialFrame;assembly=Sharpnado.MaterialFrame"
             x:Class="PsyApp.App">
    <Application.Resources>
        <Style x:Key="AcrylicFrame" TargetType="sharpnado:MaterialFrame">
            <Setter Property="MaterialTheme" Value="AcrylicBlur"/>
            <Setter Property="LightThemeBackgroundColor" Value="#F1F1F1"/>
        </Style>
        <Style x:Key="AcrylicFrame2" TargetType="sharpnado:MaterialFrame">
            <Setter Property="MaterialTheme" Value="AcrylicBlur"/>
            <Setter Property="Elevation" Value="3"/>
            <Setter Property="LightThemeBackgroundColor" Value="#FFFFFF"/>
        </Style>
        <Style TargetType="NavigationPage">
            <Setter Property="BarBackgroundColor" Value="#274029"/>
            <Setter Property="BarTextColor" Value="White"/>
        </Style>
    </Application.Resources>
</Application>